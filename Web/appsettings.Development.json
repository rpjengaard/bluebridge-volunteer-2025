{
  "$schema": "appsettings-schema.json",
  "EmailSettings": {
    "SmtpHost": "localhost",
    "SmtpPort": 25,
    "EnableSsl": false,
    "FromEmail": "noreply@bluebridge.dk",
    "FromName": "Blue Bridge Frivillig"
  },
  "uSync": {
    "Settings": {
      "ExportOnSave": "Settings",
      "ImportAtStartup": "Once",
      "ExportAtStartup": "None",
      "RebuildCacheOnCompletion": true
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "UseHttps": false
      },
      "Content": {
        "MacroErrors": "Throw"
      },
      "Runtime": {
        "Mode": "Development"
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto"
      },
      "Hosting": {
        "Debug": true
      },
      "WebRouting": {
        "UmbracoApplicationUrl": "https://localhost:44304"
      }
    }
  }
}
